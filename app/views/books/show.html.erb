<p id="notice">Book was successfully created.</p>


<% @books.each do |book| %>
    <tr>
      <td><strong>Title</strong><%= @list.title %></td>
      <td><strong>Body</strong><%= @list.body %></td>
      <td><%= link_to 'Edit', edit_path(book.id) %></td>
      <td><%= link_to 'Back', "/books" %></td>
    </tr>
<% end %>